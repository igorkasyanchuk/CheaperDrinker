= content_for :on_ready do
  init_add_form(); 
  init_location_form();
  
= error_messages_for :location

= semantic_form_for([:dashboard, current_user, @location]) do |f|
  = f.inputs do
    = render :partial => '/dashboard/locations/inputs', :locals => { :f => f }
  = f.buttons do
    = f.submit f.object.new_record? ? 'Create' : "Update"