= f.input :location_id, :as => :select, :collection => current_user.locations
= f.input :title
= f.input :description, :input_html => { :rows => '12', :cols => '80' }
= f.input :photo
= f.input :start, :as => :string, :input_html => { :class => 'date_picker' }
= f.input :end, :include_blank => true, :as => :string, :input_html => { :class => 'date_picker' }
= f.input :start_time, :include_blank => true

= add_city_name_autocomplete("location_city")